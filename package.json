{
  "name": "aramsay-express-router",
  "version": "1.0.5",
  "description": "Express routes configured using TypeScript decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm test && rimraf dist && tsc --project tsconfig.json",
    "test": "cross-env JASMINE_CONFIG_PATH=jasmine.json jasmine-ts src/**/*.spec.ts",
    "autotest": "nodemon --watch src/**/*.* --exec npm test"
  },
  "keywords": [
    "TypeScript",
    "decorators",
    "express",
    "router"
  ],
  "author": "Andrew Ramsay <aramsaybusiness@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewaramsay/aramsay-express-router.git"
  },
  "devDependencies": {
    "cross-env": "^3.1.2",
    "jasmine": "^2.5.2",
    "jasmine-ts": "0.0.3",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "reflect-metadata": "^0.1.8",
    "typescript": "^2.0.3"
  }
}
